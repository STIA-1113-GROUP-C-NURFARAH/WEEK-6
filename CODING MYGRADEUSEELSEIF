package mySwitch;
import java.util.Scanner;
public class MyGradeUseIfElse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner (System.in);
		

		
		System.out.println("Enter grade from A,B,C,D,E and F:");
		String str = sc.next();
		char grade = str.charAt(0);
		
		if (grade == 'A')
			System.out.println("Grade A is marks >= 90");
		else if (grade == 'B')
			System.out.println("Grade B ismarks >= 80 and less than 90");
		else if (grade == 'C')
			System.out.println("Grade C is marks >= 70 and less than 80");
		else if (grade == 'D')
			System.out.println("Grade D is marks >= 60 and less than 70");
		else if (grade == 'E')
			System.out.println("Grade E is marks >= 50 and less than 60");
		else if (grade == 'F')
			System.out.println("Grade F is marks <= 40");
		else 
			System.out.println("invalid grade");
	}

}
